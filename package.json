{
	"name": "olyop",
	"type": "module",
	"description": "olyop",
	"license": "UNLICENSED",
	"author": {
		"name": "Oliver Plummer",
		"email": "oliver.plummer@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/olyop/olyop.git"
	},
	"scripts": {
		"sync": "git checkout . && git remote update && git pull",
		"prepush": "npm run build",
		"push": "git add -A && git commit -m \"Update\" && git push -u origin main",
		"init": "clear",
		"prebuild": "npm run init",
		"build": "npm-run-all build:*",
		"build:static": "cpy src/public .",
		"build:webpack": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider -r dotenv/config\" webpack --config webpack.config.ts",
		"prestart": "npm run init && kill-port 3000",
		"start": "cross-env NODE_OPTIONS=\"--experimental-specifier-resolution=node --openssl-legacy-provider -r dotenv/config\" webpack serve --config webpack.config.ts"
	},
	"devDependencies": {
		"@oly_op/eslint-config": "5.1.6",
		"@oly_op/stylelint-config": "2.0.4",
		"@oly_op/tsconfig": "1.0.7",
		"@types/dotenv-webpack": "7.0.3",
		"@types/eslint": "8.2.2",
		"@types/mini-css-extract-plugin": "2.4.0",
		"@types/node": "17.0.8",
		"@types/webpack": "5.28.0",
		"@types/webpack-dev-server": "4.5.1",
		"@types/webpack-env": "1.16.3",
		"@types/write-file-webpack-plugin": "4.5.2",
		"@typescript-eslint/eslint-plugin": "5.9.0",
		"@typescript-eslint/parser": "5.9.0",
		"ajv": "8.8.2",
		"compression-webpack-plugin": "9.2.0",
		"copy-webpack-plugin": "10.2.0",
		"cpy-cli": "3.1.1",
		"cross-env": "7.0.3",
		"css-loader": "6.5.1",
		"css-minimizer-webpack-plugin": "3.3.1",
		"del-cli": "4.0.1",
		"dotenv": "10.0.0",
		"dotenv-webpack": "7.0.3",
		"eslint": "8.6.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "16.1.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "6.0.0",
		"html-webpack-plugin": "5.5.0",
		"kill-port": "1.6.1",
		"mini-css-extract-plugin": "2.4.6",
		"npm-run-all": "4.1.5",
		"sass": "1.47.0",
		"sass-loader": "12.4.0",
		"source-map-loader": "3.0.1",
		"style-loader": "3.3.1",
		"stylelint": "14.2.0",
		"stylelint-scss": "4.1.0",
		"ts-loader": "9.2.6",
		"ts-node": "10.4.0",
		"typescript": "4.6.0-dev.20220107",
		"webpack": "5.65.0",
		"webpack-cli": "4.9.1",
		"webpack-dev-server": "4.7.2",
		"write-file-webpack-plugin": "4.5.1"
	}
}
