{
	"name": "olyop",
	"type": "module",
	"description": "olyop",
	"license": "UNLICENSED",
	"author": {
		"name": "Oliver Plummer",
		"email": "oliver.plummer@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/olyop/olyop.git"
	},
	"scripts": {
		"sync": "git checkout . && git remote update && git pull",
		"push": "git add -A && git commit -m \"Update\" && git push -u origin main",
		"init": "clear",
		"prebuild": "del build",
		"host": "cross-env NODE_OPTIONS=\"-r dotenv/config\" serve build",
		"build": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack -c webpack.config.ts",
		"prestart": "npm run init && kill-port 3000",
		"start": "cross-env NODE_OPTIONS=\"-r dotenv/config\" webpack serve -c webpack.config.ts",
		"build-host": "npm run build && npm run host",
		"script:upload-to-s3": "node --loader ts-node/esm -r dotenv/config scripts/upload-to-s3.ts",
		"upload-to-s3": "npm-run-all init build script:upload-to-s3"
	},
	"devDependencies": {
		"@oly_op/eslint-config": "5.1.14",
